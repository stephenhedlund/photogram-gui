<ul>
  <li>
    <a href="/users">Users</a>
  </li>
  <li>
    <a href="/photos">Photos</a>
  </li>
</ul>

<h1>List of photos</h1>

<hr>

<table border="1">
<tr>
  <th>
    Image
  </th>
  <th>
    Caption
  </th>
  <th>
    Owner
  </th>
  <th>
    Posted
  </th>
  <th>
  </th>
</tr>

<% @list_of_photos.each do |a_photo| %>
<tr>
  <td>
    <img src="<%=a_photo.id %>">
  </td>
  <td>
    <%=a_photo.caption%>
  </td>
  <td>
    <%=a_photo.owner_id%>
  </td>
  <td>
    <%=a_photo.created_at%>
  </td>
  <td>
    <a href="/photos/ <%=a_photo.id%>">
    Show details
  </td>
</tr>
<% end %>
</table>
